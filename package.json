{
  "name": "asset-hash",
  "version": "2.0.0",
  "description": "Very fast asset hashing function for using e.g. during front-end deployments.",
  "main": "lib/index.cjs.js",
  "module": "lib/index.esm.js",
  "scripts": {
    "lint:js": "eslint --ignore-path .gitignore .",
    "fix:js": "eslint --ignore-path .gitignore --fix .",
    "format:js": "prettier --ignore-path .gitignore --write '**/*.{js,mjs}' && npm run fix:js",
    "release": "git push && release-it --github.release --npm.publish --non-interactive",
    "release:minor": "git push && release-it --github.release --npm.publish --non-interactive --increment minor",
    "release:major": "git push && release-it --github.release --npm.publish --non-interactive --increment major",
    "prepare": "preppy",
    "test": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sebastian-software/asset-hash.git"
  },
  "keywords": [
    "asset",
    "front-end",
    "hash",
    "base62",
    "xxhash"
  ],
  "files": [
    "lib/"
  ],
  "author": {
    "name": "Sebastian Werner",
    "email": "s.werner@sebastian-software.de",
    "url": "http://sebastian-software.de/werner"
  },
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.4.0",
    "babel-preset-edge": "^4.9.1",
    "eslint": "^5.1.0",
    "eslint-config-readable": "^2.1.3",
    "flow-bin": "^0.76.0",
    "jest": "^23.4.1",
    "preppy": "^4.2.3",
    "prettier": "^1.13.7"
  },
  "dependencies": {
    "@babel/runtime": "^7.0.0-beta.53",
    "big.js": "^5.1.2",
    "core-js": "^2.5.7",
    "hash-through": "^0.1.16",
    "metrohash": "^2.4.1",
    "xxhash": "^0.2.4"
  }
}
